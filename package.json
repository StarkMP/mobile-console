{
  "name": "mobile-console",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "version": "1.0.0",
  "description": "Development browser console for mobile devices",
  "scripts": {
    "dev": "webpack serve --mode development --node-env development --config tools/webpack.development.js",
    "build": "webpack --mode production --node-env production --config tools/webpack.production.js && tsc --noEmit false --emitDeclarationOnly",
    "typecheck": "tsc",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint": "npm run eslint && npm run typecheck",
    "test": "jest",
    "prepublishOnly": "npm run lint & npm run build & npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StarkMP/mobile-console.git"
  },
  "keywords": [
    "devtools",
    "mobile console",
    "phone console",
    "phone devtools",
    "mobile devtools"
  ],
  "author": "Maksim Serebryakov <starkmp.work@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StarkMP/mobile-console/issues"
  },
  "homepage": "https://github.com/StarkMP/mobile-console#readme",
  "baseUrl": "",
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "devDependencies": {
    "@babel/core": "7.22.15",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-runtime": "7.22.15",
    "@babel/preset-env": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@jest/globals": "29.6.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-loader": "8.3.0",
    "clean-webpack-plugin": "4.0.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-webpack-plugin": "5.5.3",
    "jest": "28.1.3",
    "prettier": "2.8.8",
    "typescript": "4.9.5",
    "webpack": "5.88.2",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "3.11.3"
  }
}
